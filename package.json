{
  "author": "hunzsig",
  "buildConfig": {
    "appHtml": {
      "app": "app.html",
      "dev": "dev.html",
      "prod": "index.html"
    },
    "dropConsole": true,
    "entry": {
      "app": "src/app.js",
      "dev": "src/index.js",
      "prod": "src/index.js"
    },
    "import": [
      {
        "libraryName": "antd-mobile",
        "libraryDirectory": "es/components",
        "style": false
      }
    ],
    "publicUrl": {
      "app": "./",
      "dev": "/",
      "prod": "/"
    }
  },
  "copyConfig": {
    "except": [],
    "include": []
  },
  "dependencies": {
    "antd-mobile": "5.8.0",
    "antd-mobile-icons": "latest",
    "axios": "0.26.1",
    "classnames": "2.2.6",
    "crypto-js": "^3.1.9-1",
    "enquire-js": "0.2.1",
    "foundation-symbol": "0.1.4",
    "highlight.js": "11.5.0",
    "moment": "2.29.1",
    "nanoid": "2.1.11",
    "prop-types": "15.8.1",
    "react-color": "2.19.3",
    "react-contextmenu": "2.14.0",
    "react-document-title": "2.0.3",
    "react-json-view": "1.21.3",
    "script-loader": "0.7.2"
  },
  "description": "魂书，一个简介爽快写书项目",
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "babel-plugin-import": "1.7.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "hunzsig-javascript-cli-v16": "latest",
    "react": "18.0.0-rc.3",
    "react-dom": "18.0.0-rc.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://www.hunzsig.org",
  "name": "Hunlook",
  "scripts": {
    "app": "cliv16 app",
    "build": "cliv16 build",
    "lint": "eslint . --ext '.js,.jsx' --fix",
    "start": "cliv16 dev -p 9711"
  },
  "title": "魂书 Hunlook",
  "version": "0.2.0"
}
